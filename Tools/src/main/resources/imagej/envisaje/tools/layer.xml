<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="brushes">
	<attr name="SystemFileSystem.localizingBundle"
	    stringvalue="imagej.envisaje.tools.Bundle" />

        <file name="imagej-envisaje-tools-brushes-StandardBrush.instance">
            <attr name="SystemFileSystem.localizingBundle"
                stringvalue="imagej.envisaje.tools.Bundle" />
            <attr name="SystemFileSystem.icon" urlvalue=
	        "nbresloc:/imagej/envisaje/tools/resources/standardbrush.png"/>
        </file>
    </folder>
    <folder name="fills">
	<attr name="SystemFileSystem.localizingBundle"
	    stringvalue="imagej.envisaje.tools.Bundle" />
	    
        <file name="imagej-envisaje-tools-fills-SolidColorFill.instance">
            <attr name="SystemFileSystem.localizingBundle" 
                stringvalue="imagej.envisaje.tools.Bundle" />
            <attr name="SystemFileSystem.icon" urlvalue=
	        "nbresloc:/imagej/envisaje/tools/resources/solidfill.png"/>
        </file>
        <file name="imagej-envisaje-tools-fills-GradientFill.instance">
            <attr name="SystemFileSystem.localizingBundle"
                stringvalue="imagej.envisaje.tools.Bundle" />
            <attr name="SystemFileSystem.icon" urlvalue=
	        "nbresloc:/imagej/envisaje/tools/resources/gradientfill.png"/>
        </file>
        
        <file name="TestFill.instance" url="resources/smudge.png">
            <attr name="SystemFileSystem.icon" urlvalue="nbfs:/SystemFileSystem/fills/TestFill.instance"/>
            <attr name="instanceClass" stringvalue="imagej.envisaje.tools.fills.PatternFill"/>
            <attr name="instanceCreate" methodvalue="imagej.envisaje.tools.fills.PatternFill.create"/>
        </file>
        
    </folder>
    
    <folder name="brushtips">
	<attr name="SystemFileSystem.localizingBundle"
	    stringvalue="imagej.envisaje.tools.Bundle" />
	<file name="imagej-envisaje-tools-brushes-tips-RoundBrushTip.instance">
