<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
	<!-- this means that files with a .ImageFile extension are recognized by this loader -->
       <ext name="jpeg"/>
       <ext name="JPEG"/>
       <ext name="jpg"/>
       <ext name="JPG"/>
       <ext name="jpe"/>
       <ext name="tif"/>
       <ext name="tiff"/>
       <ext name="png"/>
       <ext name="gif"/>
       <resolver mime="image/x-ImageFile"/>
   </file>
</MIME-resolver>
