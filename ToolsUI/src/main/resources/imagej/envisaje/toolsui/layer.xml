<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="imagej-envisaje-toolsui-ToolsAction.instance"/>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="ToolsAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/imagej-envisaje-toolsui-ToolsAction.instance"/>
            </file>
        </folder>
        <!-- Add Top-level Tools Menu -->
        <attr name="imagej-envisaje-toolsui-ToolsMenu.instance/Window" boolvalue="true"/>
        <file name="imagej-envisaje-toolsui-ToolsMenu.instance"/>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="ToolsTopComponent.settings" url="ToolsTopComponent_settings.xml"/>
        </folder>
        <folder name="Modes">
            <file name="explorer.wsmode" url="explorer.wsmode"/>
            <folder name="explorer">
                <file name="ToolsTopComponent.wstcref" url="ToolsTopComponent_tcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
